{
  "name": "home-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "cross-fetch": "3.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.11.5",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.3",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "react-scripts": "2.1.8",
    "typescript": "3.3.4000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "prettier --single-quote --print-width=120 --parser typescript --write",
        "git add"
      ],
      "*.{css, json}": [
        "prettier --write",
        "git add"
      ]
    }
  }
}
